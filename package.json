{
  "name": "pack2",
  "version": "0.1.0",
  "description": "mini pack tool",
  "main": "src/index.js",
  "bin": {
    "pack2": "./bin/pack2.js"
  },
  "scripts": {
    "commit": "npm run git:add && npm run git:commit",
    "git:add": "git add .",
    "git:commit": "git-cz",
    "commitizen:init": "commitizen init cz-conventional-changelog --save --save-exact",
    "lint:js": "eslint --ext .js bin/ src/ --format codeframe",
    "lint:js:fix": "eslint --fix --ext .js bin/ src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Royal-Tramp/pack2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Royal-Tramp/pack2/issues"
  },
  "homepage": "https://github.com/Royal-Tramp/pack2#readme",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/parser": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/traverse": "^7.11.5",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.2",
    "commander": "^6.1.0",
    "fs-extra": "^9.0.1",
    "uglify-js": "^3.10.4"
  },
  "devDependencies": {
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
